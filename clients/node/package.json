{
  "name": "@retab/node",
  "version": "1.0.83",
  "description": "Retab official Node.js library",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:verbose": "bun test --reporter verbose",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retab-dev/retab"
  },
  "keywords": [
    "retab",
    "sdk",
    "api",
    "client"
  ],
  "author": "Retab",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "axios-retry": "^4.0.0",
    "blake2": "^5.0.0",
    "dotenv": "^17.2.0",
    "file-type": "^21.0.0",
    "form-data": "^4.0.4",
    "js-yaml": "^4.1.0",
    "openai": "^5.8.2",
    "zod": "^3.22.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/blake2": "^4.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
